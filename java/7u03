#%Module1.0
#
proc ModulesHelp { } {
        puts stderr "\tInitializes your environment to use java jdk1.7.0_03"
}

module-whatis "initializes java"

setenv JAVA_HOME /srv/gs1/software/java/jdk1.7.0_03
prepend-path PATH /srv/gs1/software/java/jdk1.7.0_03/bin
prepend-path JRE_HOME /srv/gs1/software/java/jdk1.7.0_03/jre
prepend-path MANPATH /srv/gs1/software/java/jdk1.7.0_03/man
